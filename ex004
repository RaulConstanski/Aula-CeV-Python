variavel = input('Digite algo: ')
print('O tipo primitivo deste valor é ', type(variavel))
print('É numerico? ',(variavel.isnumeric()))
print('É alfabeto? ',(variavel.isalpha()))
print('É alfanumerico', variavel.isalnum())
print('Esta em maiuscula?', variavel.isupper())
print('Esta em minuscula?', variavel.islower())
print('Esta capitalizada, primeiro letra maiuscula?', variavel.istitle())
print('Só tem espaços?', variavel.isspace())
